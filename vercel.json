{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "^/$",
      "dest": "/home.html"
    },
    {
      "src": "^/([^/]+)$",
      "dest": "/$1.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}